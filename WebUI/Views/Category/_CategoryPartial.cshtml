@model WebUI.Models.Category.ModelForCategoryPartial

@{ string ConvertPicture(byte[] picture)
    {
        var base64 = Convert.ToBase64String(picture);
        return $"data:image/jpg;base64,{base64}";
    } }

<td class="product-price">@Model.Category.Id</td>
<td class="product-thumbnail"><img src="@ConvertPicture(Model.Category.Picture)" /></td>
<td class="product-name">@Model.Category.Name</td>
<td class="product-remove">
    <a href="#" data-toggle="modal" data-target="#updateCategoryModal" data-name="@Model.Category.Name" data-elementid="@Model.ElementId"
       data-description="@Model.Category.Description" data-picture="@ConvertPicture(Model.Category.Picture)" data-id="@Model.Category.Id">
        <i class="fa fa-pencil"></i>
    </a>
</td>
<td class="product-remove">
    <a style="cursor:pointer" onclick="jQueryAjaxDelete('@Url.Action("Delete", "Category", new { id = Model.Category.Id }, Context.Request.Scheme)', '@Model.ElementId')">
        <i class="fa fa-times"></i>
    </a>
</td>